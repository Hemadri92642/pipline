pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Good morning') {
            steps {
                echo 'Good morning'
            }
        }
        stage('age') {
            steps {
                echo '30'
            }
        }
        stage('Name') {
            steps {
                echo 'Hemadri'
            }
        }
    }
}

